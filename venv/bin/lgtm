#!/Users/TakahiroKurokawa/Python実践入門/Py-Practice/ch13/workspace/venv/bin/python3 -x
# EASY-INSTALL-ENTRY-SCRIPT: 'lgtm','console_scripts','lgtm'
__requires__ = 'lgtm'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('lgtm', 'console_scripts', 'lgtm')()
    )
